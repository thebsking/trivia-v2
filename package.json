  "bugs": {
    "url": "https://github.com/thebsking/trivia-v2/issues"
  },
  "homepage": "https://github.com/thebsking/trivia-v2#readme",
  "dependencies": {
    "body-parser": "^1.20.0",
    "concurrently": "^7.2.2",
    "express": "^4.18.1",
    "mongoose": "^6.4.2"
  }
}
